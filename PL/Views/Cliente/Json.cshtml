@page
@{
    ViewBag.Title = "Json";
    //Layout = "~/Views/Shared/_Layout.cshtml";
    Layout = null;
}

<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
<link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
<link rel="stylesheet" href="~/PL.styles.css" asp-append-version="true" />
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">
<script src="https://use.fontawesome.com/releases/v6.1.0/js/all.js" crossorigin="anonymous"></script>
<br />
<br />
<div class="container">
    @using Microsoft.AspNetCore.Http;
    @inject IHttpContextAccessor HttpContextAccessor
    @{
        string Json = HttpContextAccessor.HttpContext.Session.GetString("Json");
    }
    <div style="float:left;">
        @Html.ActionLink("Descargar Registro", "JsonToExcel", "Cliente", htmlAttributes: new { @class = "btn btn-info bi bi-filetype-XML" })
    </div>
    <a type="button" asp-area="" asp-controller="Home" asp-action="Index">Ir a Menú</a>
    <br /><br />
    <body>
        <a>@Json</a>
    </body>

</div>
